  <script id="com-yesasia-yesstyle-web-wicket-panel-browse-filter-range-Range-0" type="text/javascript">
  function applyRange(target,min,max,current) {
	  var rs=$(target).prev(".rangeselection");
    $(".minValue",rs).html(current[0]);
    $(".maxValue",rs).html(current[1]);      
    var rf = $(target).nextAll("form");  
    $(target).slider({
      range: true,
      min: min,
      max: max,
      values: current,
      slide: function(event, ui) {        
        $(".minValue",rs).html(ui.values[0]);
        rf.find(".startValue").attr("value", ui.values[0]);
        $(".maxValue",rs).html(ui.values[1]);
        rf.find(".endValue").attr("value", ui.values[1]);
      },
      change: function(event, ui) {
    	  $(".minValue",rs).html(ui.values[0]);
        rf.find(".startValue").attr("value", ui.values[0]);
        $(".maxValue",rs).html(ui.values[1]);
        rf.find(".endValue").attr("value", ui.values[1]);
        if (ui.values[1]>=max){
$(".plus",rs).show();
if (ui.values[0]==min){
  $(target).siblings("a.empty").hide();
}else{
  $(target).siblings("a.empty").show();
}  
        }else{
$(".plus",rs).hide();
$(target).siblings("a.empty").show();
        } 
    	}   
    }).next("a.empty").unbind().click(function(){
      var t=$(target);
      t.slider("values",0,min);
      t.slider("values",1,max);
      $(this).toggle();
    })
    if ($(target).slider("values",1)>=max){
      $(".plus",rs).show();
      if ($(target).slider("values",0)==min){
        $(target).siblings("a.empty").hide();
      }else{
        $(target).siblings("a.empty").show();
      }
    }else{
      $(".plus",rs).hide();
      $(target).siblings("a.empty").show();
    }
  }
  </script>  
